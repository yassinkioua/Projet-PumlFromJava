@startuml
'https://plantuml.com/sequence-diagram
skinparam style strictuml

participant MyDoclet.class as DocletC
participant Doclet


[o-> DocletC ++ : main(args)

DocletC -->> Doclet ++ : <<Create>> Doclet()
return Doclet = Doclet()


DocletC -> Doclet : init(local, reporter)

DocletC -> Doclet++ : getSupportedOptions()
return Set<Doclet.options> SupportedOptions

DocletC -> Doclet++ : getSupportedSourceVersion()
return  JavaVersionSupported : SourceVersion

DocletC -> Doclet++ : run(environment : DocletEnvironment)

alt "run is success"
    return true
end
@enduml

